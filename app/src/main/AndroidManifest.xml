<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leier.personalandroidtest.personalandroidtestproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImageShowFrame.Picasso.SampleTestPicassoActivity"
            android:label="@string/title_activity_sample_test_picasso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NetConnectionFrame.OkHttp.SampleTestOkHttpActivity"
            android:label="@string/title_activity_sample_test_ok_http"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>